    "@types/js-cookie": "^2.2.6",
    "@types/node": "^15.0.3",
    "@types/showdown": "^1.9.3",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "chart.js": "3.2.1",
    "chartjs-plugin-datalabels": "v2.0.0-beta.1",
    "eslint": "^7.26.0",
    "jquery": "3.6.0",
    "js-cookie": "v3.0.0-rc.1",
    "jsmin": "^1.0.1",
    "showdown": "1.9.1",
    "socket.io": "4.0.2",
    "socket.io-client": "4.0.2",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@airframes/acars-decoder": "^1.1.0",
